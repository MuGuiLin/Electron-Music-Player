{
  "name": "electron-music-player",
  "version": "1.0.0",
  "author": "沐枫",
  "description": "本地、网络 - 音乐播放器",
  "main": "main.js",
  "license": "CC0-1.0",
  "scripts": {
    "start": "chcp 65001 && electron .",
    "build": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.mupiao.app",
    "productName": "Mupiao",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "static/icon.ico"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "Music Play"
  ],
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-store": "^5.0.0",
    "needle": "^2.5.0"
  }
}